{
  "name": "u-home-server",
  "version": "1.0.0",
  "description": "Server for your home application",
  "main": "server/index.js",
  "author": "tamnk",
  "license": "ISC",
  "scripts": {
    "start": "nodemon .`",
    "server": "node  ./dist/index.js",
    "dev": "nodemon --legacy-watch ./server/index.js",
    "build": "babel  ./server --out-dir dist --copy-files",
    "test": "echo \"Error: no test specified\" && exit 1",
    "pm2:start": "pm2 start ./dist/index.js --name blog",
    "pm2:stop": "pm2 stop ./dist/index.js",
    "prettier": "prettier --check **/*.js",
    "prettier:fix": "prettier --write **/*.js",
    "lint:fix": "eslint ./server --fix",
    "lint": "eslint ./server",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bull": "^3.16.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "ejs": "^2.7.4",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.10",
    "express-validation": "^1.0.3",
    "fs-extra": "^7.0.1",
    "google-auth-library": "^6.0.6",
    "http-status": "^1.4.2",
    "install": "^0.13.0",
    "joi": "^14.3.1",
    "jsonapi-serializer": "^3.6.6",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "mysql2": "^2.1.0",
    "npm": "^6.14.7",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pm2": "^4.4.0",
    "querystring": "^0.2.0",
    "request": "^2.88.2",
    "sequelize": "^5.22.3",
    "sequelize-cli": "^5.5.1",
    "url": "^0.11.0",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "gulp-babel": "^8.0.0",
    "html-webpack-plugin": "^4.4.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5"
  }
