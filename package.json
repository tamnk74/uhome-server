{
  "name": "u-home-server",
  "version": "0.0.2",
  "description": "Server for your home application",
  "main": "server/index.js",
  "author": "tamnk",
  "license": "ISC",
  "scripts": {
    "start": "nodemon .",
    "server": "pm2 start ecosystem.config.js --ignore-watch --no-daemon",
    "dev": "concurrently \"nodemon --legacy-watch ./server/index.js\" \"nodemon --legacy-watch ./server/schedulers\"",
    "build": "babel  ./server --out-dir dist --copy-files",
    "test": "echo \"Error: no test specified\" && exit 1",
    "pm2:start": "pm2 start ./dist/index.js --name blog",
    "pm2:stop": "pm2 stop ./dist/index.js",
    "prettier": "prettier --check **/*.js",
    "prettier:fix": "prettier --write **/*.js",
    "lint:fix": "eslint ./server --fix",
    "lint": "eslint ./server",
    "setup": "npx sequelize db:migrate",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up -d",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "prettier --write",
      "git add",
      "eslint"
    ]
  },
  "_moduleAliases": {
    "@": "./server"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.47.0",
    "@aws-sdk/s3-request-presigner": "^3.47.0",
    "@googlemaps/google-maps-services-js": "^3.3.4",
    "@sentry/node": "^6.3.5",
    "aws-sdk": "^2.863.0",
    "axios": "^0.21.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bull": "^3.16.0",
    "bull-arena": "^3.15.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.10.4",
    "dotenv": "^7.0.0",
    "ejs": "^2.7.4",
    "excel4node": "^1.7.2",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.10",
    "express-validation": "^1.0.3",
    "fast-json-stringify": "^2.7.6",
    "firebase-admin": "^9.5.0",
    "form-data": "^4.0.0",
    "fs-extra": "^7.0.1",
    "google-auth-library": "^6.0.6",
    "http-status": "^1.5.0",
    "i18n": "^0.13.2",
    "install": "^0.13.0",
    "ioredis": "^4.23.0",
    "joi": "^14.3.1",
    "jsencrypt": "^3.2.1",
    "jsonapi-serializer": "^3.6.6",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "minio": "^7.0.18",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "mysql2": "^2.1.0",
    "node-rsa": "^1.0.5",
    "node-schedule": "^2.0.0",
    "npm": "^6.14.7",
    "otplib": "^12.0.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pm2": "^4.4.0",
    "qs": "^6.10.2",
    "querystring": "^0.2.0",
    "request": "^2.88.2",
    "sequelize": "^5.22.3",
    "sequelize-cli": "^5.5.1",
    "twilio": "^4.6.0",
    "url": "^0.11.0",
    "uuid": "^3.4.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.1.4",
    "gulp-babel": "^8.0.0",
    "html-webpack-plugin": "^4.4.1",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5"
  }
}
